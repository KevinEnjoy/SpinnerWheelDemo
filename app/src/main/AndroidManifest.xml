<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo.spinnerwheeldemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TimePickerActivity" android:label="Time picker">
        </activity>
        <activity android:name=".TimePickerCustomViewsActivity" android:label="Time picker with custom views">
        </activity>
        <activity android:name=".ProgrammaticSwitchingActivity" android:label="Programmatic switching (shuffle)">
        </activity>
        <activity android:name=".CitiesActivity" android:label="Repopulating data">
        </activity>
    </application>

</manifest>
